				},
				"getAnswer(uint256)": {
					"params": {
						"_roundId": "the answer number to retrieve the answer for"
					}
				},
				"getTimestamp(uint256)": {
					"params": {
						"_roundId": "the answer number to retrieve the updated timestamp for"
					}
				},
				"pluginCallback(bytes32,int256)": {
					"details": "This function can only be called by the oracle that received the request.",
					"params": {
						"_clRequestId": "The Plugin request ID associated with the answer",
						"_response": "The answer provided by the Plugin node"
					}
				},
				"renounceOwnership()": {
					"details": "Allows the current owner to relinquish control of the contract."
				},
				"requestData(address)": {
					"details": "This example does not include request parameters. Reference any documentation associated with the Job IDs used to determine the required parameters per-request."
				},
				"setAuthorization(address,bool)": {
					"params": {
						"_allowed": "boolean that determines whether the requester is permissioned or not",
						"_requester": "the address whose permissions are being set"
					}
				},
				"transferOwnership(address)": {
					"details": "Allows the current owner to transfer control of the contract to a newOwner.",
					"params": {
						"_newOwner": "The address to transfer ownership to."
					}
				},
				"transferPLI(address,uint256)": {
					"details": "The contract will need to have a PLI balance in order to create requests.",
					"params": {
						"_amount": "The amount of PLI to send from the contract",
						"_recipient": "The address to receive the PLI tokens"
					}
				},
				"updateRequestDetails(uint128,address[],string[])": {
					"details": "Arrays are validated to be equal length.",
					"params": {
						"_jobIds": "An array of Job IDs",
						"_minimumResponses": "the minimum number of responses before an answer will be calculated",
						"_oracles": "An array of oracle addresses"
					}
				}
			},
			"title": "An example Plugin contract with aggregation"
		},
		"userdoc": {
			"methods": {
				"cancelRequest(bytes32,uint256,uint256)": {
					"notice": "Cancels an outstanding Plugin request. The oracle contract requires the request ID and additional metadata to validate the cancellation. Only old answers can be cancelled."
				},
				"destroy()": {
					"notice": "Called by the owner to kill the contract. This transfers all PLI balance and ETH balance (if there is any) to the owner."
				},
				"getAnswer(uint256)": {
					"notice": "get past rounds answers"
				},
				"getOracleResponses()": {
					"notice": "get the most recently reported answer"
				},
				"getTimestamp(uint256)": {
					"notice": "get block timestamp when an answer was last updated"
				},
				"latestAnswer()": {
					"notice": "get the most recently reported answer"
				},
				"latestRound()": {
					"notice": "get the latest completed round where the answer was updated"
				},
				"latestTimestamp()": {
					"notice": "get the last updated at block timestamp"
				},
				"pluginCallback(bytes32,int256)": {
					"notice": "Receives the answer from the Plugin node."
				},
				"renounceOwnership()": {
					"notice": "Renouncing to ownership will leave the contract without an owner. It will not be possible to call the functions with the `onlyOwner` modifier anymore."
				},
				"requestData(address)": {
					"notice": "Creates a Plugin request for each oracle in the oracles array."
				},
				"setAuthorization(address,bool)": {
					"notice": "Called by the owner to permission other addresses to generate new requests to oracles."
				},
				"transferPLI(address,uint256)": {
					"notice": "Allows the owner of the contract to withdraw any PLI balance available on the contract."
				},
				"updateRequestDetails(uint128,address[],string[])": {
					"notice": "Updates the arrays of oracles and jobIds with new values, overwriting the old values."
				}
			}
		}
