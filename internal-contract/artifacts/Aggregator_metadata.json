			"methods": {
				"cancelRequest(bytes32,uint256,uint256)": {
					"params": {
						"_expiration": "is the time when the request expires",
						"_payment": "is the amount of PLI paid to the oracle for the request",
						"_requestId": "is the identifier for the plugin request being cancelled"
